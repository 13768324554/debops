# {{ ansible_managed }}

#
# PRODUCTION
#
production:
  adapter: mysql2
  encoding: utf8
  reconnect: false
  database: {{ gitlab_database_config[gitlab_database].database }}
  pool: 10
  username: {{ gitlab_database_config[gitlab_database].username }}
  password: "{{ gitlab_database_password }}"
  host: {{ gitlab_database_config[gitlab_database].hostname }}
  # socket: /tmp/mysql.sock


